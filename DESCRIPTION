Package: pixelclasser
Type: Package
Title: Classifies Image Pixels by Colour
Version: 1.0.0
Authors@R: c(person("Carlos", "Real", email = "carlos.real@usc.es",  role = c("aut", "cre"), comment = c(ORCID = "0000-0002-5433-6728")),
